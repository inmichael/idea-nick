{
  "name": "@ideanick/client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "types": "tsc --noEmit --project ./tsconfig.app.json && tsc --noEmit --project ./tsconfig.node.json",
    "preview": "vite preview",
    "prettify": "prettier --loglevel warn --cache '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts .tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache src/**/*.scss --fix"
  },
  "dependencies": {
    "@ideanick/server": "workspace:*",
    "@tanstack/react-query": "^5.74.3",
    "@trpc/client": "^11.1.0",
    "@trpc/react-query": "^11.1.0",
    "include-media": "^2.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.5.1",
    "reset-css": "^5.0.2",
    "sass": "^1.86.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "stylelint": "^16.18.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.0"
  }
}
