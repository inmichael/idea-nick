  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.1",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
